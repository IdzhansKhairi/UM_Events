    <TextView
        android:id="@+id/TVtitleSEV"
        android:layout_width="245dp"
        android:layout_height="42dp"
        android:layout_marginStart="85dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/kadwa"
        android:text="Comic Fiesta 2022"
        android:textColor="@color/white"
        android:textSize="26dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/TVBackSEV"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/detail_bg"
        android:layout_width="400dp"
        android:layout_height="90dp"
        android:layout_marginTop="440dp"
        android:src="@drawable/detail_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/comicfiestaposter"
        android:layout_width="402dp"
        android:layout_height="235dp"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="4dp"
        android:src="@drawable/comic_fiesta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <TextView
        android:id="@+id/desc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="4dp"
        android:fontFamily="@font/kadwa"
        android:text="@string/SEV2"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/comicfiestaposter" />

    <ImageView
        android:id="@+id/whiteline2"
        android:layout_width="385dp"
        android:layout_height="2dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="12dp"
        android:src="@drawable/line_bg"
        app:layout_constraintBottom_toTopOf="@+id/detail_bg"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/TVBackSEV"
        android:layout_width="61dp"
        android:layout_height="39dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/kadwa"
        android:text="Back"
        android:textColor="@color/white"
        android:textSize="26dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/TVtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="56dp"
        android:fontFamily="@font/kadwa"
        android:text="@string/time"
        android:textSize="10dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/detail_bg"
        app:layout_constraintEnd_toEndOf="@+id/detail_bg" />

    <TextView
        android:id="@+id/TVlocation"
        android:layout_width="174dp"
        android:layout_height="21dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="48dp"
        android:fontFamily="@font/kadwa"
        android:text="@string/location"
        android:textSize="10dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/detail_bg"
        app:layout_constraintTop_toTopOf="@+id/detail_bg" />

    <TextView
        android:id="@+id/TVdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/kadwa"
        android:text="@string/date"
        android:textSize="10dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/detail_bg"
        app:layout_constraintTop_toTopOf="@+id/detail_bg" />

    <TextView
        android:id="@+id/TVwebsite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/kadwa"
        android:text="@string/website"
        android:textSize="10dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/detail_bg"
        app:layout_constraintEnd_toEndOf="@+id/detail_bg" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:layout_marginBottom="104dp"
        android:foregroundGravity="center_horizontal"
        app:cardCornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_bg">


        <ImageView
            android:id="@+id/map"
            android:layout_width="400dp"
            android:layout_height="80dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="104dp"
            android:scaleType="fitXY"
            android:src="@drawable/mapcomicfiesta"
            android:onClick="openMap"/>
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/next_to_heart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/kadwa"
        android:text="@string/set"
        android:textColor="@color/white"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView">

    </TextView>

    <ImageView
        android:id="@+id/tag_bg"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="680dp"
        android:src="@drawable/tag_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tags"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="684dp"
        android:fontFamily="@font/kadwa"
        android:text="Tags:"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/carnival"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="292dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/kadwa"
        android:text="Carnival"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/showcase"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/kadwa"
        android:text="Showcase"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/whiteline"
        android:layout_width="385dp"
        android:layout_height="2dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/line_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/comicfiestaposter" />

    <ImageView
        android:id="@+id/ic_calendar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/ic_calendar"
        app:layout_constraintEnd_toStartOf="@+id/TVdate"
        app:layout_constraintTop_toTopOf="@+id/detail_bg" />

    <ImageView
        android:id="@+id/ic_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="22dp"
        android:src="@drawable/ic_location"
        app:layout_constraintBottom_toBottomOf="@+id/detail_bg"
        app:layout_constraintStart_toStartOf="@+id/detail_bg" />

    <ImageView
        android:id="@+id/ic_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="156dp"
        android:src="@drawable/ic_time"
        app:layout_constraintEnd_toEndOf="@+id/detail_bg"
        app:layout_constraintTop_toTopOf="@+id/detail_bg" />

    <ImageView
        android:id="@+id/ic_website"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="24dp"
        android:src="@drawable/ic_website"
        app:layout_constraintBottom_toBottomOf="@+id/detail_bg"
        app:layout_constraintEnd_toEndOf="@+id/detail_bg" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="28dp"
        android:button="@drawable/selector_wishlist_item"
        app:layout_constraintBottom_toTopOf="@+id/tags"
        app:layout_constraintEnd_toStartOf="@+id/next_to_heart" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="39dp"
        android:layout_height="37dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/ic_baseline_arrow_back_ios_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>